    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "stylelint": "^16.2.0",
    "stylelint-config-standard": "^36.0.0",
